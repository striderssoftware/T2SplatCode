= element VGridSeedSouth isa QSeed
\symbol vs
\color #0f3
#\audio #000
#\symmetries all

== Data members
u Unsigned count = 0u;

== Rules
given @ isa VGridSeedSouth{
. DebugUtils db;
. $self.count++;
. if ( $self.count >= $self.size )
. {
.  Junction j;
.  ew[2] = j;
.  $self.count = 0u;
. }
. else
. {
.  Grid newgrid;
.  ew[2] = newgrid;
. }
.
.}


given j isa Junction
given g isa Grid
let c = j|g

   c     c
   @     _
   _  -> @
